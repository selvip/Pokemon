<%= form_for(@pokemon) do |f| %>
	<%= f.label 'Pokedex', for: 'pokedex_id' %>
	<%= f.select(:pokedex_id, @list_names_ids, {}, class: 'form-control form-group') %>
	
	<%= (content_tag(:div, flash[:notice] , class: 'alert alert-danger', role: 'alert')) if flash[:notice] %>

	<%= f.submit 'Add Pokemon', class: 'btn btn-primary form-group' %>
<% end %>